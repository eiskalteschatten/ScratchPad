          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Debido a la seguridad moderna de macOS, no es posible importar automáticamente desde ScratchPad 1.x. Sigue los pasos a continuación para continuar."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
